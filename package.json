{
    "name": "cpp-code-actions",
    "displayName": "C++ Code Actions",
    "description": "Custom Context Menu Code Actions For C++",
    "version": "0.0.1",
    "publisher": "Rett",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.CreateDefinition"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.CreateDefinition",
                "title": "Say Hello"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "resourceLangId == cpp",
                    "command": "extension.CreateDefinition",
                    "group": "YourGroup@1"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}